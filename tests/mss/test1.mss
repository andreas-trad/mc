.section1:showPososta{
    backgroundColorBlue: @domel.data-pososto;
    backgroundColorGreen: @domel.data-pososto;
    backgroundColorRed: @domel.data-pososto;
    duration:@domel.duration;
    width:@domel.data-pososto;
    easing: swing;
	
	complete
	{
		span
		{
			duration:500;
			opacity:1;
		}
	}
}
