.giftbox:TiltBox{
    duration:300;
    easing: swing;
	rotateZ: 30deg;
}

.giftbox:ResetBox
{
    duration:200;
    easing: swing;
	rotateZ: 0deg;
}

.giftdiv:Advertisement
{
    .giftbox
    {
    	duration:200;
    	opacity:0;
    	height:0;
    }
    
    .emailcontainer
    {
    	delay:300;
    	opacity:1;
    	duration:200;
    }
    
    
}

